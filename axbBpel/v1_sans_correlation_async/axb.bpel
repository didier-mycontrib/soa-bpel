<!-- axb BPEL Process [Generated by the Eclipse BPEL Designer] -->
<!-- Date: Mon Oct 01 11:19:24 CEST 2012 -->
<bpel:process name="axb"
         targetNamespace="http://tp"
         suppressJoinFailure="yes"
         xmlns:tns="http://tp"
         xmlns:nsc="http://service.tp/"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         >

    <!-- Import the client WSDL -->
	<bpel:import location="axbArtifacts.wsdl" namespace="http://tp" 
	        importType="http://schemas.xmlsoap.org/wsdl/" />
	        
    <bpel:import location="calculateur.wsdl" namespace="http://service.tp/" 
	        importType="http://schemas.xmlsoap.org/wsdl/" />
	        
    <bpel:import location="myRemoteLogger.wsdl" namespace="http://service.tp/" 
	        importType="http://schemas.xmlsoap.org/wsdl/" />     	        	        
         
    <!-- ================================================================= -->         
    <!-- PARTNERLINKS                                                      -->
    <!-- List of services participating in this BPEL process               -->
    <!-- ================================================================= -->         
    <bpel:partnerLinks>
        <!-- The 'client' role represents the requester of this service. -->
        <bpel:partnerLink name="client"
                     partnerLinkType="tns:axb"
                     myRole="axbProvider"
                     />
         <bpel:partnerLink name="calculateur"
                     partnerLinkType="nsc:calculateur"
                     partnerRole="roleCalculateur"
                     />  
          <bpel:partnerLink name="myRemoteLogger"
                     partnerLinkType="nsc:myRemoteLogger"
                     partnerRole="myRemoteLogger"
                     />                       
         <!-- nsc defini dans bpel:process via xmlns:nsc="http://service.tp/" 
              correspond au targetNameSpace du service calculateur  -->            
         <!-- partnerLinkType &  partnerRole comme dans wsdl -->                               
    </bpel:partnerLinks>
  
    <!-- ================================================================= -->         
    <!-- VARIABLES                                                         -->
    <!-- List of messages and XML documents used within this BPEL process  -->
    <!-- ================================================================= -->         
    <bpel:variables>
        <!-- Reference to the message passed as input during initiation -->
        <bpel:variable name="input"
                  messageType="tns:axbRequestMessage"/>
                  
        <bpel:variable name="additionInput"  messageType="nsc:addition"/>
        <bpel:variable name="additionOutput" messageType="nsc:additionResponse"/>  
        
        <bpel:variable name="multiplicationInput"  messageType="nsc:multiplication"/>
        <bpel:variable name="multiplicationOutput" messageType="nsc:multiplicationResponse"/>                                   
                  
         <bpel:variable name="logMessageInput"  messageType="nsc:logMessage"/>   
         <bpel:variable name="testCorrelationInput"  messageType="nsc:testCorrelation"/>                   
        <!-- 
          Reference to the message that will be returned to the requester
          -->
        <bpel:variable name="output"
                  messageType="tns:axbResponseMessage"/>
    </bpel:variables>

    <!-- ================================================================= -->         
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->         
    <bpel:sequence name="main">
        
        <!-- Receive input from requester. 
             Note: This maps to operation defined in axb.wsdl 
             -->
        <bpel:receive name="receiveInput" partnerLink="client"
                 portType="tns:axb"
                 operation="process_axb" variable="input"
                 createInstance="yes"/>
        
        <!-- Generate reply to synchronous request -->
        <bpel:assign validate="no" name="Assign1">
            
            
           
            <bpel:copy>
                <bpel:from>
                    <bpel:literal xml:space="preserve"><tns:multiplication xmlns:tns="http://service.tp/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <a>0.0</a>
  <b>0.0</b>
</tns:multiplication>
</bpel:literal>
                </bpel:from>
                <bpel:to part="parameters" variable="multiplicationInput"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from>
                    
                    
                    <![CDATA[$input.payload/tns:a]]>
                </bpel:from>
                <bpel:to part="parameters" variable="multiplicationInput">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[a]]></bpel:query>
                </bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from>
                    
                    
                    <![CDATA[$input.payload/tns:x]]>
                </bpel:from>
                <bpel:to part="parameters" variable="multiplicationInput">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[b]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:invoke name="InvokeMultiplication" partnerLink="calculateur" operation="multiplication" portType="nsc:Calculateur" inputVariable="multiplicationInput" outputVariable="multiplicationOutput"></bpel:invoke>
        <bpel:assign validate="no" name="Assign2">
            
            <bpel:copy>
                <bpel:from>
                    <bpel:literal xml:space="preserve"><tns:addition xmlns:tns="http://service.tp/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <a>0.0</a>
  <b>0.0</b>
</tns:addition>
</bpel:literal>
                </bpel:from>
                <bpel:to part="parameters" variable="additionInput"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from>
                    
                    
                    <![CDATA[$multiplicationOutput.parameters/return]]>
                </bpel:from>
                <bpel:to part="parameters" variable="additionInput">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[a]]></bpel:query>
                </bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from>
                    
                    
                    <![CDATA[$input.payload/tns:b]]>
                </bpel:from>
                <bpel:to part="parameters" variable="additionInput">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[b]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:invoke name="InvokeAddition" partnerLink="calculateur" operation="addition" portType="nsc:Calculateur" inputVariable="additionInput" outputVariable="additionOutput"></bpel:invoke>
        <bpel:assign validate="no" name="Assign3">
            <bpel:copy>
                <bpel:from>
                    <bpel:literal xml:space="preserve"><tns:axbResponse xmlns:tns="http://tp" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:result>0.0</tns:result>
</tns:axbResponse>
</bpel:literal>
                </bpel:from>
                <bpel:to part="payload" variable="output"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from>
                    
                    
                    
                    <![CDATA[$additionOutput.parameters/return]]>
                </bpel:from>
                <bpel:to part="payload" variable="output">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:result]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:flow name="Flow_en_parallele">
        
        <bpel:sequence name="Sequence_sync">
         
        <bpel:reply name="replyOutput" 
               partnerLink="client"
               portType="tns:axb"
               operation="process_axb" 
               variable="output"
               />
        </bpel:sequence>
        <bpel:sequence name="Sequence_async">
        <bpel:assign validate="no" name="AssignLogMsg">
            <bpel:copy>
                <bpel:from>
                    <bpel:literal xml:space="preserve"><tns:logMessage xmlns:tns="http://service.tp/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <msg>msg</msg>
</tns:logMessage>
</bpel:literal>
                </bpel:from>
                <bpel:to part="parameters" variable="logMessageInput"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from>
                    
                    
                    
                    <![CDATA[concat('a*x plus b=' , $output.payload/tns:result)]]>
                </bpel:from>
                <bpel:to part="parameters" variable="logMessageInput">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[msg]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:invoke name="InvokeLogMessage" partnerLink="myRemoteLogger" operation="logMessage" portType="nsc:MyRemoteLogger" inputVariable="logMessageInput"></bpel:invoke>
        </bpel:sequence>
        </bpel:flow>
       
    </bpel:sequence>
</bpel:process>

